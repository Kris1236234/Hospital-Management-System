@model Hospital.ViewModels.DoctorViewModel

@{
  ViewData["Title"] = "Details";
}



<h4>Szczegóły o lekarzu</h4>
<hr />
<div class="row">
  <div class="col-md-4">
    <div class="form-group">
      <label asp-for="Name" class="control-label">@Model.Name</label>
    </div>
        <div class="form-group">
            <label asp-for="LastName" class="control-label">@Model.LastName</label>
        </div>
    <div class="form-group">
      <label asp-for="Specialization" class="control-label">@Model.Specialization</label>
    </div>
        <div class="form-group">
            <label asp-for="PWZNumber" class="control-label">@Model.PWZNumber</label>
        </div>
        <div class="form-group">
            <label asp-for="SortBy" class="control-label">@Model.SortBy</label>
        </div>
  </div>
</div>

<div>
  <a asp-action="Index">Powrót do listy</a>
</div>


