@model Hospital.ViewModels.HospitalInfoViewModel

@{
  ViewData["Title"] = "Details";
}



<h4>Szczegóły Szpitalu</h4>
<hr />
<div class="row">
  <div class="col-md-4">
    <div class="form-group">
      <label asp-for="Name" class="control-label">@Model.Name</label>
    </div>
        <div class="form-group">
            <label asp-for="Type" class="control-label">@Model.Type</label>
        </div>
    <div class="form-group">
      <label asp-for="City" class="control-label">@Model.City</label>
    </div>
        <div class="form-group">
            <label asp-for="PinCode" class="control-label">@Model.PinCode</label>
        </div>
        <div class="form-group">
            <label asp-for="Country" class="control-label">@Model.Country</label>
        </div>
  </div>
</div>

<div>
  <a asp-action="Index">Powrót do listy</a>
</div>


